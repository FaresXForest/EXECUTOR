exec.MouseButton1Click:Connect(function()
    local s = box.Text
    if s and s ~= "" then
        local f, err = loadstring(s)
        if f then
            pcall(setfenv(f, getfenv()))
            pcall(f)
        else
            warn(err)
        end
    end
end)

clr.MouseButton1Click:Connect(function()
    box.Text = ""
end)

